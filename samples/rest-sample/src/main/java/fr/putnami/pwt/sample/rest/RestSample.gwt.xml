<?xml version="1.0" encoding="UTF-8"?>
 <module rename-to="RestSample">
	<!-- PWT Module to extends in order to use PWT widgets-->
	<inherits name="fr.putnami.pwt.core.widget.Widget"/>
	<!-- PWT Module to extends in order to use PWT MVP features-->
	<inherits name="fr.putnami.pwt.core.mvp.Mvp"/>
    <!-- PWT Module to extends in order to use PWT Model features-->
    <inherits name="fr.putnami.pwt.core.model.Model"/>
    <!-- PWT Module to extends in order to use PWT Service features-->
	<inherits name="fr.putnami.pwt.core.service.Service"/>
	<!-- PWT Module to extends in order to use PWT Inject features-->
	<inherits name="fr.putnami.pwt.core.inject.Inject"/>
	<!-- PWT Module to extends in order to use PWT Rest features-->
	<inherits name="fr.putnami.pwt.plugin.rest.RestService"/>

     <entry-point class="fr.putnami.pwt.sample.rest.client.RestSampleApp"/>

     <!-- This linker will be the default one in later version of GWT -->
     <add-linker name="xsiframe" />

     <source path="client"/>
     <source path="shared"/>

     <!-- Add the fr locale -->
     <extend-property name="locale" values="fr"/>
 </module>
