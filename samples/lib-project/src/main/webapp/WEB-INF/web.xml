<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <!-- Servlets -->
  <servlet>
    <servlet-name>commandService</servlet-name>
    <servlet-class>fr.putnami.pwt.core.service.server.service.BasicCommandService</servlet-class>
    <init-param>
        <param-name>services</param-name>
        <param-value>fr.putnami.pwt.example.sample.server.service.ContactServiceImpl;fr.putnami.pwt.example.sample.server.service.ErrorServiceImpl</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>commandService</servlet-name>
    <url-pattern>Sample/commandService</url-pattern>
  </servlet-mapping>

</web-app>