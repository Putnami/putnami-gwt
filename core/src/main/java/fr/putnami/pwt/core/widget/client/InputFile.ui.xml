<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:fr.putnami.pwt.core.widget.client"
	>


	<g:HTMLPanel>
		<g:FormPanel ui:field="uploadForm" method="post" encoding="multipart/form-data" action="uploadfile">
			<g:FileUpload ui:field="fileUpload" name="data" />
		</g:FormPanel>
		<w:OutputProgressBar ui:field="progressBar" displayValue="true" visible="false" />
		<w:Label ui:field="fileName" stylePrimaryName="fileName" />
		<w:Button ui:field="downloadBtn" type="ICON" size="LARGE" iconType="DOWNLOAD"/>
		<w:Button ui:field="deleteBtn" type="ICON" size="LARGE" iconType="CANCEL"/>
		<w:Button ui:field="uploadBtn" type="ICON" size="LARGE" iconType="UPLOAD"/>
	</g:HTMLPanel>
</ui:UiBinder>